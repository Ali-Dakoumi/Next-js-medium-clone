{
  "id": "blogpost",
  "label": "Blog Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Uid",
          "placeholder": ""
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "img": {
        "type": "Image",
        "config": {
          "label": "Img",
          "constraint": {},
          "thumbnails": []
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "author": {
        "type": "StructuredText",
        "config": {
          "label": "Author",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "category",
          "placeholder": "",
          "options": [
            "tactics",
            "news"
          ]
        }
      }
    },
    "meta": {
      "Categories": {
        "type": "Group",
        "config": {
          "label": "Categories",
          "fields": {
            "Category": {
              "type": "Link",
              "config": {
                "label": "Category",
                "select": "document",
                "customtypes": [
                  "category"
                ]
              }
            }
          }
        }
      }
    }
  }
}